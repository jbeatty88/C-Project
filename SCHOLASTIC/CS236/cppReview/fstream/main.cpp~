#include <iostream>
#include <fstream>

using namespace std;

int main(int argc, char** argv) {
  string fName;
  
  if(argc == 1){
    cout << "Please include an input file" << endl;
    exit(0);
  }

  fName = argv[1];
  cout << "File name: " << fName << endl;
 
    

  
  cout << argv[0] << endl;
  cout << argv[1] << endl;
  


  return 0;
}
